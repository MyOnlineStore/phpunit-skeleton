{
    "name": "myonlinestore/phpunit-skeleton",
    "type": "library",
    "description": "A tool to create phpunit skeletons for your files",
    "homepage": "https://github.com/MyOnlineStore",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\PhpUnitSkeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\PhpUnitSkeleton\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2.0",
        "roave/better-reflection": "~3",
        "symfony/console": "^3.0"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3.4",
        "kherge/box": "^2.7",
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.2"
        }
    },
    "bin": ["phpunit-skeleton.phar"]
}
